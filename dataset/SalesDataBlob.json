				"type": "String"
			},
			{
				"name": "Gender",
				"type": "String"
			},
			{
				"name": "Age",
				"type": "String"
			},
			{
				"name": "Product Category",
				"type": "String"
			},
			{
				"name": "Quantity",
				"type": "String"
			},
			{
				"name": "Price per Unit",
				"type": "String"
			},
			{
				"name": "Total Amount",
				"type": "String"
			}
		]
